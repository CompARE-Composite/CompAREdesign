Delta_IC <- function(SD,n,alpha){
LI <- SD^2*(n-1)/qchisq(1-alpha/2,n-1)
LS <- SD^2*(n-1)/qchisq(  alpha/2,n-1)
return(LS-LI)
}
Delta_IC <- function(n,SD,alpha,target_width){
LI <- SD^2*(n-1)/qchisq(1-alpha/2,n-1)
LS <- SD^2*(n-1)/qchisq(  alpha/2,n-1)
return(abs((LS-LI)-target_width))
}
